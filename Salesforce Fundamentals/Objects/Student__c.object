    <validationRules>
        <fullName>Check_Credit_Card_Number_Format</fullName>
        <active>true</active>
        <description>Validates that a custom text field called Credit_Card_Number is formatted in 9999-9999-9999-9999 or 9999999999999999 number format when it is not blank.</description>
        <errorConditionFormula>NOT( REGEX( Credit_Card_Number__c , &quot;(((\\d{4}-){3}\\d{4})|\\d{16})?&quot;))</errorConditionFormula>
        <errorDisplayField>Credit_Card_Number__c</errorDisplayField>
        <errorMessage>Credit Card Number format must be 9999-9999-9999-9999 or 9999999999999999.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Weekday</fullName>
        <active>true</active>
        <description>Validates that the value of a custom date field is a weekday (not Saturday or Sunday).</description>
        <errorConditionFormula>CASE(MOD( Demo_Date__c - DATE(1900, 1, 7), 7), 0, 0, 6, 0, 1) = 0</errorConditionFormula>
        <errorDisplayField>Demo_Date__c</errorDisplayField>
        <errorMessage>Date must be a weekday.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Multiple_of_Five</fullName>
        <active>true</active>
        <description>Validates that a custom field called Multiple of 5 is a multiple of five.</description>
        <errorConditionFormula>MOD( Multiple_of_5__c, 5) &lt;&gt; 0</errorConditionFormula>
        <errorDisplayField>Multiple_of_5__c</errorDisplayField>
        <errorMessage>Number must be a multiple of five.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_DOB</fullName>
        <active>true</active>
        <description>Checks that Student DOB should not be greater than TODAY.</description>
        <errorConditionFormula>DOB__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>DOB__c</errorDisplayField>
        <errorMessage>DOB should not be greater than TODAY.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Mix_Pct</fullName>
        <active>true</active>
        <description>Validates that a custom field called Mix Pct is between 0 and 100%.</description>
        <errorConditionFormula>OR(
   Mix_Pct__c &gt; 1.0,
   Mix_Pct__c &lt; 0.0
)</errorConditionFormula>
        <errorDisplayField>Mix_Pct__c</errorDisplayField>
        <errorMessage>Mix Pct must be between 0 and 100%.</errorMessage>
    </validationRules>
